<mapper namespace="mappers.user">

	<!-- 회원가입 -->
	<insert id="registerUser">
		insert into user userId = #{userId}, password = #{password}
	</insert>

	<!-- 아이디 찾기? 아이디조회? -->
	<select id="findByUserId" resultType="com.example.Gesuipan.entity.User">
		select * from user where userId = #{userId}
	</select>

</mapper>